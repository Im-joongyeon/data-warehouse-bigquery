{
  "name": "bigquery-sink-transactions",
  "config": {
    "connector.class": "com.wepay.kafka.connect.bigquery.BigQuerySinkConnector",
    "tasks.max": "1",
    "topics": "dbserver1.public.transactions",
    "sanitizeTopics": "true",
    "autoCreateTables": "false",
    "autoUpdateSchemas": "true",
    "schemaRetriever": "com.wepay.kafka.connect.bigquery.retrieve.IdentitySchemaRetriever",
    "project": "${GCP_PROJECT_ID}",
    "defaultDataset": "kafka_ingestion",
    "keyfile": "/tmp/keyfile.json",
    "keySource": "FILE",
    "table.name.format": "transactions",
    "bigQueryMessageTimePartitioning": "false",
    "bigQueryPartitionDecorator": "false",
    "allowNewBigQueryFields": "true",
    "allowBigQueryRequiredFieldRelaxation": "true",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "transforms": "removeNullFields",
    "transforms.removeNullFields.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.removeNullFields.blacklist": "updated_at",
    "deleteEnabled": "false",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "behavior.on.null.values": "ignore",
    "consumer.override.auto.offset.reset": "earliest"
  }
}
